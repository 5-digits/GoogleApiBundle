{
    "name": "happyr/google-api-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony2 Wrapper for the Google APIs Client Library for PHP",
    "keywords": ["Google API"],
    "homepage": "http://developer.happyr.se/symfony2-bundles/google-api-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Tobias Nyholm",
            "email": "tobias@happyrecruiting.se"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "2.*",
        "symfony/yaml": "2.*",
        "nyholm/google-api-php-client": "0.1.*"
    },
    "require-dev": {
        "mockery/mockery": "*"
    },
    "autoload": {
        "psr-0": { "HappyR\\Google\\ApiBundle": "" }
    },
    "target-dir": "HappyR/Google/ApiBundle"
    
}
